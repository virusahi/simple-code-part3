#include <stdio.h>

// c program to check whether a number is even or odd.  

int main() {

    int num;

    printf(" Enter any numerical integer: ");

    scanf("%d", &num);

    // true if num is perfectly divisible by 2

    if(num % 2 == 0)

      printf("%d is an even integer.", num);

    else

        printf("%d is an odd integer.", num);


    return 0;

}
